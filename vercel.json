{
    "builds": [{
        "src": "tributepage/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "includeFiles": "static/**"
        }
    }],
    "routes": [
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*)", "dest": "tributepage/wsgi.py" }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "tributepage.settings"
    }
}