{
    "builds": [{
        "src": "tributepage/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": {
            "maxLambdaSize": "15mb", "runtime": "python3.8"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tributepage/wsgi.py"
        }
    ]
}